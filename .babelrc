{
  "presets": [
    ["latest",
      {
        "es2015": {
          "modules": false
        }
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel",
    ["module-resolver", {
      "root": ["./src"]
    }]
  ],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
